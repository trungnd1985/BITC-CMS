@model IList<int>

@(Html.Kendo().MultiSelectFor(m => m)
                .DataTextField("TagName")
                .DataValueField("BlogTagID")
                .AutoBind(true)
                //.Value(Model)
                .DataSource(source =>
                {
                    source.Read(read =>
                    {
                        read.Action("LoadAllBlogTags", "BlogTag");
                    });
                })

)
